''' MySQL Data Tracer

This script traces all MySQL data on the cluster.
'''

import px


def pgsql_data(start: str, num_head: int):
    df = px.DataFrame(table='pgsql_events', start_time=start)

    # Replace UPID with pod.
    df.pod = df.ctx['pod']
    df = df.drop(['upid', 'trace_role'])

    # Restrict number of results.
    df = df.head(num_head)

    return df
